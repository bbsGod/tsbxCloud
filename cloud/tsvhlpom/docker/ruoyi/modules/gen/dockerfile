# 基础镜像
FROM  openjdk:8-jre
# author
MAINTAINER tsvhlpom

# 挂载目录
VOLUME /home/tsvhlpom
# 创建目录
RUN mkdir -p /home/tsvhlpom
# 指定路径
WORKDIR /home/tsvhlpom
# 复制jar文件到路径
COPY ./jar/tsvhlpom-modules-gen.jar /home/tsvhlpom/tsvhlpom-modules-gen.jar
# 启动代码生成服务
ENTRYPOINT ["java","-jar","tsvhlpom-modules-gen.jar"]
